CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE EMP(EMP_ID INTEGER,DEPT_ID INTEGER,EMP_FIRST_NAME VARCHAR(255),LAST_NAME VARCHAR(255))
CREATE MEMORY TABLE DEPT(DEPT_ID INTEGER,DEPT_NAME VARCHAR(255))
CREATE MEMORY TABLE SCOTT_EMP(EMPNO SMALLINT,ENAME VARCHAR(10),JOB VARCHAR(9),MGR SMALLINT,HIREDATE TIMESTAMP,SAL NUMERIC(7,2),COMM NUMERIC(7,2),DEPTNO SMALLINT,NEWCOL DOUBLE)
CREATE MEMORY TABLE SCOTT_DEPT(DEPTNO SMALLINT,DNAME VARCHAR(14),LOC VARCHAR(13))
CREATE MEMORY TABLE DEPARTMENT(DEPARTMENT_KEY INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,DEPARTMENT_ID VARCHAR(50),DEPARTMENT_NAME VARCHAR(100),EMPLOYEE_COUNT INTEGER,CREATED_TS TIMESTAMP DEFAULT CURRENT_TIMESTAMP,UPDATED_TS TIMESTAMP DEFAULT CURRENT_TIMESTAMP)
CREATE MEMORY TABLE EMPLOYEE(EMPLOYEE_KEY INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,EMPLOYEE_ID VARCHAR(50),EMPLOYEE_NAME VARCHAR(100),DEPARTMENT_ID VARCHAR(50),DEPARTMENT_NAME VARCHAR(100),CREATED_TS TIMESTAMP DEFAULT CURRENT_TIMESTAMP,UPDATED_TS TIMESTAMP DEFAULT CURRENT_TIMESTAMP)
ALTER TABLE DEPARTMENT ALTER COLUMN DEPARTMENT_KEY RESTART WITH 3
ALTER TABLE EMPLOYEE ALTER COLUMN EMPLOYEE_KEY RESTART WITH 14
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
SET WRITE_DELAY 10
SET SCHEMA PUBLIC
